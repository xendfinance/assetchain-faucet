    "nyc": "^15.1.0",
    "sinon": "^15.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
